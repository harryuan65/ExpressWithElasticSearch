{
  "name": "express-with-es",
  "version": "1.0.0",
  "description": "harry",
  "main": "app.js",
  "watches": {
    "sass": "sass/**"
  },
  "scripts": {
    "sass-generate": "node-sass ./public/stylesheets/scss -o ./public/stylesheets/translated-css/",
    "prestart": "yarn sass-generate",
    "start": "node app.js",
    "server": "nodemon --watch ./public/stylesheets/scss/style.scss --watch app.js --watch src --exec npm start"
  },
  "author": "harry",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^7.6.1",
    "ejs": "^3.1.2",
    "express": "^4.17.3",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.1"
  },
  "repository": "none",
  "private": false,
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
